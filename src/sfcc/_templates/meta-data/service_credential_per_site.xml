    <service-credential service-credential-id="b2ccrmsync.{{B2C_SITEID}}">
        <url>https://{{SF_LOGINURL}}/services/oauth2/token</url>
        <user-id>{{SF_USERNAME}}</user-id>
        <password>{{SF_PASSWORD}}{{SF_SECURITYTOKEN}}</password>
        <custom-attributes>
            <custom-attribute attribute-id="consumerKey">{{SF_CONSUMERKEY}}</custom-attribute>
            <custom-attribute attribute-id="consumerSecret">{{SF_CONSUMERSECRET}}</custom-attribute>
        </custom-attributes>
    </service-credential>
